@using ServicePro.BAL;
@using ServicePro
@model CompanyMaster
@{
    ViewBag.Title = "Detail";
}
<!-- jQuery 2.2.3 -->
<script src="/UI/plugins/jQuery/jquery-2.2.3.min.js"></script>
<script src="/Scripts/Master/CompanyMaster.js"></script>
<input type="hidden" id="hdIsActive" value="@Model.IsActive" />
<input type="hidden" id="hdCompanyMaster_ID" value="@Model.CompanyMaster_ID" />
<input type="hidden" id="hdActivationMaster_Key" value="@Model.ActivationMaster_Key" />

<div class="container-fluid">
    <div class="col-md-offset-2 col-md-8 col-sm-12" style="padding-top:80px;">
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Company Name
                </label>
            </div>
            <div class="col-sm-8">
                <input type="text" value="@Model.CompanyName" class="form-control" id="CompanyName">
            </div>
        </div>
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Company Type
                </label>
            </div>
            <div class="col-sm-8">
                <input type="text" value="@Model.CompanyType" class="form-control" id="CompanyType">
            </div>
        </div>
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Tin No
                </label>
            </div>
            <div class="col-sm-8">
                <input type="text" value="@Model.TinNo" class="form-control" id="TinNo">
            </div>
        </div>
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    UserName
                </label>
            </div>
            <div class="col-md-8">
                <input type="text" value="@Model.UserName" class="form-control" id="UserName">
            </div>
        </div>
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Password
                </label>
            </div>
            <div class="col-md-8">
                <input type="text" value="@Model.Password" class="form-control" id="Password">
            </div>
        </div>
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Recovery Mobile
                </label>
            </div>
            <div class="col-md-8">
                <input type="text" value="@Model.Recovery_Mobile" class="form-control" id="Recovery_Mobile">
            </div>
        </div>
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Recovery Email
                </label>
            </div>
            <div class="col-md-8">
                <input type="text" value="@Model.Recovery_Email" class="form-control" id="Recovery_Email">
            </div>
        </div>
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Recovery Question
                </label>
            </div>
            <div class="col-md-8">
                <input type="text" value="@Model.Recovery_Question" class="form-control" id="Recovery_Question">
            </div>
        </div>
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Recovery Answer
                </label>
            </div>
            <div class="col-md-8">
                <input type="text" value="@Model.Recovery_Answer" class="form-control" id="Recovery_Answer">
            </div>
        </div>
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Company Logo
                </label>
            </div>
            <div class="col-md-8">
                <input type="text" value="@Model.CompanyLogo" class="form-control" id="CompanyLogo">
            </div>
        </div>
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Report Signature
                </label>
            </div>
            <div class="col-md-8">
                <input type="text" value="@Model.ReportSignature" class="form-control" id="ReportSignature">
            </div>
        </div>
        <div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Report Bottom
                </label>
            </div>
            <div class="col-md-8">
                <input type="text" value="@Model.ReportBottom" class="form-control" id="ReportBottom">
            </div>
        </div>
        @*<div class="row form-group">
            <div class="col-sm-3">
                <label class="control-label">
                    Active
                </label>
            </div>
            <div class="col-sm-8">
                <input type="checkbox" class="checkbox" id="IsActive" />
            </div>
        </div>*@
    </div>
</div>


<!-- CompanyMaster Grid Starts here -->
<div class="container-fluid fill" id="CompanyMasterGridList" style="display:none;">
    <br />
    <div class="row">
        <div class="col-md-offset-8 col-md-4">
            <div class="input-group">
                <input type="text" id="searchCompanyMaster" class="form-control" placeholder="Search for...">
                <span class="input-group-btn">
                    <button type="button" id="btnCompanySearch" class="btn btn-default">Search</button>
                </span>
            </div>
        </div>
    </div>
    <br />
    <table id="CompanyMasterGrid" data-source="@Url.Action("GetCompanyMaster")"></table>
</div>
<!-- CompanyMaster Grid End here -->


<!-- AddressMaster Grid Starts here -->

    <div class="container-fluid fill" id="AddressGridList" style="display:none;">
